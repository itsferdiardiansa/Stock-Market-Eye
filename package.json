{
  "name": "stock-market-eye",
  "version": "0.1.0",
  "private": true,
  "author": {
    "email": "ferdiardiansa@gmail.com",
    "name": "ferdiardiansa"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "install:clean": "pnpm run clean && pnpm i",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "pnpm run lint --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "format:check": "pnpm run format --check",
    "test": "jest --passWithNoTests",
    "test:watch": "pnpm run test --watch",
    "test:coverage": "pnpm run test --coverage",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist/ coverage/ .next/ node_modules/ pnpm-lock.yaml",
    "validate": "pnpm lint && pnpm type-check && pnpm format:check && pnpm test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "pnpm run format",
      "pnpm run test"
    ],
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@react-jvectormap/core": "^1.0.4",
    "@react-jvectormap/world": "^1.1.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/postcss": "^4.0.9",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "apexcharts": "^4.3.0",
    "autoprefixer": "^10.4.20",
    "clsx": "^1.1.1",
    "flatpickr": "^4.6.13",
    "lint-staged": "^11.0.0",
    "next": "15.1.3",
    "object-path": "^0.11.5",
    "path": "^0.12.7",
    "prettier": "^2.3.2",
    "react": "^19.0.0",
    "react-apexcharts": "^1.7.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "react-flatpickr": "^3.10.13",
    "sass": "1.32.1",
    "swiper": "^11.2.0",
    "tailwind-merge": "^2.6.0"
    
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/chart.js": "2.9.32",
    "@types/node": "^20",
    "@types/nouislider": "15.0.0",
    "@types/object-path": "0.11.0",
    "@types/prismjs": "1.16.5",
    "@types/react": "^19",
    "@types/react-copy-to-clipboard": "5.0.0",
    "@types/react-dom": "^19",
    "@types/react-flatpickr": "^3.8.11",
    "@types/react-redux": "7.1.16",
    "@types/react-transition-group": "^4.4.12",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "tailwindcss": "^4",
    "postcss": "^8",
    "typescript": "^5"
  }
